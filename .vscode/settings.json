{
	"search.exclude": {
		"export/**/*.hx": true
	},
	"[haxe]": {
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
		"editor.codeActionsOnSave": {
			"source.sortImports": "explicit"
		}
	},
	"haxe.enableExtendedIndentation": true,
	"files.associations": {
		"algorithm": "cpp",
		"iterator": "cpp",
		"xhash": "cpp",
		"xtree": "cpp",
		"xutility": "cpp"
	}
}